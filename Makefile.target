obj-mips-y += piix4.o parallel.o cirrus_vga.o pcspk.o $(sound-obj-y)
obj-mips-y += mipsnet.o ne2000-isa.o
obj-mips-y += pflash_cfi01.o
obj-mips-y += vmware_vga.o
obj-mips-$(CONFIG_AR7) += ar7.o
obj-mips-$(CONFIG_PFLASH) += pflash.o
obj-mips-$(CONFIG_PFLASH) += pflash_amd.o
obj-mips-$(CONFIG_PFLASH) += pflash_cfi02.o

obj-microblaze-y = petalogix_s3adsp1800_mmu.o

obj-microblaze-y += microblaze_pic_cpu.o
obj-microblaze-y += xilinx_intc.o
obj-microblaze-y += xilinx_timer.o
