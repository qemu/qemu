config HPPA_B160L
    bool
    default y
    depends on HPPA
    imply PCI_DEVICES
    imply E1000_PCI
    imply USB_OHCI_PCI
    imply VIRTIO_VGA
    select ASTRO
    select DINO
    select LASI
    select SERIAL_MM
    select SERIAL_PCI
    select DIVA_GSP
    select ISA_BUS
    select I8259
    select IDE_CMD646
    select MC146818RTC
    select LSI_SCSI_PCI
    select PARALLEL
    select ARTIST
