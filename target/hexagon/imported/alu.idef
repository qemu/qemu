Q6INSN(A4_modwrapu,"Rd32=modwrap(Rs32,Rt32)",ATTRIBS(),
"Wrap to an unsigned modulo buffer",
{
        if (RsV < 0) {
            RdV = RsV + fCAST4u(RtV);
        } else if (fCAST4u(RsV) >= fCAST4u(RtV)) {
            RdV = RsV - fCAST4u(RtV);
        } else {
            RdV = RsV;
        }
})
