DEF_MACRO(fXORBITS,(SUM,VAR,VEC),
      "SUM = xor bitsin(VEC)",
      "XOR all bits together in a 32bit register",
      {
        for (SUM=0,VAR=0;VAR<32;VAR++) {
           SUM ^= VEC & 1;
           VEC = VEC >> 1;
        }
      },
)

