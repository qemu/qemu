		int i;
		fLOG_GLOBAL_REG_FIELD(SYSCONF,SYSCFG_TLBLOCK,0);
		for (i = 0; i < RUNNABLE_THREADS_MAX; i++) {
			if(( (thread->processor_ptr->arch_proc_options->thread_enable_mask>>i) & 0x1)) {
				thread->processor_ptr->thread[i]->cu_tlb_lock_waiting = 0;
			}
		}
      },
