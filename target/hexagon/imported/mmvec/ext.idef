EXTINSN(V6_vshuff, "vshuff(Vy32,Vx32,Rt32)",
ATTRIBS(A_EXTENSION,A_CVI,A_CVI_VP_VS),
"2x2->2x2 transpose, for multiple data sizes, inplace",
{
	fHIDE(int offset;)
	for (offset=1; offset<fVBYTES(); offset<<=1) {
		if ( RtV & offset) {
			    fHIDE(int k;) \
				fVFOREACH(8, k) {\
				if (!( k & offset)) {
					fSWAPB(VyV.ub[k], VxV.ub[k+offset]);
				}
			}
		}
	}
	})

